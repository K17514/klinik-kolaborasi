<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.klinik"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>


    <application
        android:allowBackup="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/ic_perpustaakan_round"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_perpustaakan_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.perpustakaan"
        tools:targetApi="31">
        <activity
            android:name="com.example.klinik.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.klinik.LoginActivity" />
        <activity android:name="com.example.klinik.MainActivity3" />
        <activity android:name="com.example.klinik.MainActivity2" />
        <activity android:name="com.example.klinik.MainActivity" />
        <activity android:name="com.example.klinik.EditBukuActivity" />
        <activity android:name="com.example.klinik.EditJanjiTemuActivity" />
        <activity android:name="com.example.klinik.DeleteBukuActivity" />
    </application>

</manifest>